function draw_menu()
    cls()
    cls()--clears screen
				rect(10,10,100,60,8)
				rect(10,10,101,61,12)
				--rect(x1,y1,x2,y2,color)
				print('rogue one',60,30,12)
    print("press ❎ to start",30,70,7)
end

function draw_game()
 cls(0)
 map()
 
 spr(72, p_x, p_y, 2,2)
 
 if btn(⬅️)then
 spr(96, p_x, p_y, 2,2)
 end
 
 if btn(➡️)then
 spr(64, p_x, p_y, 2,2)
 end
 
