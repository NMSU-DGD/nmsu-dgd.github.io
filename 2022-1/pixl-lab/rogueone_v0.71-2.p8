function _init()
 _upd = update_game 
 _drw = draw_game
 scene="menu"
 startgame()
 
end

function _update()
   if scene=="menu" then
        update_menu()
    elseif scene=="game" then
        _upd()
    end
end

function _draw()
  if scene=="menu" then
        draw_menu()
    elseif scene=="game" then
        _drw()
    end
end

function startgame()
  p_x = 2
  p_y = 2
end


