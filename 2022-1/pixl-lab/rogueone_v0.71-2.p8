        scene="game"
    end
end

function update_game()
 if btnp(⬅️)then
 p_x-=2 
 end
 if btnp(➡️)then
 p_x+=2
 end
 if btnp(⬇️)then
 p_y+=2
 end
 if btnp(⬆️)then
 p_y-=2
 end

 	  
 
end

