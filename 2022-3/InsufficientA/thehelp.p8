--game text

function init_text()
		
	text={}
	text.active=false
	text.str={}
	
end

function new_text(txt)
	
	text.str = txt
	text.active = true
end

function draw_text()
	
	local maxw=0
	for i=1,#text.str do
		if #text.str[i] > maxw then
			maxw = #text.str[i]
		end
	end
	
	local x = 64-maxw*2-1
	local y = 48
	local w = x+(maxw * 4)
	local h = y+#text.str*6
	
	--draw text
	
	rectfill(x-2,y-2,w+2,h+2,5)
	rectfill(x,y,w,h,6)
	
	--write text
	for i=1,#text.str do
	
		local txt= text.str[i]
		local tx = 64- #txt*2
		local ty = y - 5 + (i * 6)
		
		print(txt,tx,ty,5)
	end
	
end

