--player specific
function init_player()
 player={}
 player.x = 16
 player.y = 16
 player.speed = 1
 
 player.anim=192
 player.animct=0
 player.animspd=0.5
 player.flip = false
end

function collision(obj,d,flag)

	local x1 = 0
	local x2 = 0
	local y1 = 0
	local y2 = 0
	
 if d == "left" then
		x1 = obj.x - 1
		y1 = obj.y 
		x2 = obj.x
		y2 = obj.y + 8 - 1
 elseif d == "right" then
 	x1 = obj.x + 8 - 1
		y1 = obj.y
		x2 = obj.x + 8
		y2 = obj.y + 8 - 1
 elseif d == "up" then
 	x1 = obj.x+2
		y1 = obj.y-1
		x2 = obj.x +8 - 3
		y2 = obj.y +8 - 1
 elseif d == "down" then 
		x1 = obj.x + 2
		y1 = obj.y + 8
		x2 = obj.x + 8 -3
		y2 = obj.y + 8 
	elseif d == "center" then
		x1 = obj.x + 2
		y1 = obj.y - 3
		x2 = obj.x + 8 -3
		y2 = obj.y - 3
 end
 
 local mx = mapx *16
 local my = mapy *16
 
 x1 = flr(x1/8) + mx
 y1 = flr(y1/8) + my
 x2 = flr(x2/8) + mx
 y2 = flr(y2/8) + my
 
 
 
 if fget(mget(x1,y1),flag)
 or fget(mget(x1,y2),flag)
 or fget(mget(x2,y1),flag)
 or fget(mget(x2,y2),flag)
 then return true
 else return false
 end
 
end
